{
  "name": "projects-astro-js-template",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "// --- Development Servers ---": "",
    "dev": "astro dev --host 127.0.0.1 --port 4321",
    "dev:offline": "tsx scripts/start-offline-server.ts",
    "// --- Build & Preview ---": "",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "// --- Code Quality & Safety ---": "",
    "astro": "astro",
    "sync": "astro sync",
    "check": "astro check",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "audit:styles": "tsx scripts/audit-styles.ts",
    "// --- Cleanup ---": "",
    "clean": "rm -rf dist .astro node_modules .vercel",
    "clean:cache": "rm -rf .astro node_modules/.vite node_modules/.cache",
    "// --- Testing Pipelines (SEPARATED) ---": "",
    "test": "pnpm test:unit && pnpm test:e2e",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:offline": "playwright test --config=playwright.offline.config.ts",
    "test:e2e:ui": "start-server-and-test dev tcp:4321 'playwright test --ui'",
    "test:e2e:specific": "start-server-and-test dev tcp:4321 'playwright test'",
    "test:report": "playwright show-report",
    "test:e2e:headed": "start-server-and-test dev tcp:4321 'playwright test --headed'",
    "test:e2e:debug": "start-server-and-test dev tcp:4321 'playwright test --debug'",
    "// --- CI/CD Workflows ---": "",
    "test:ci": "playwright test",
    "validate": "pnpm check && pnpm typecheck && pnpm lint && pnpm build && pnpm test:unit && pnpm test:ci",
    "validate:quick": "pnpm check && pnpm typecheck && pnpm test:unit",
    "// --- E2E SERVER OVERRIDE (REQUIRED for Vercel Adapter) ---": "",
    "test:e2e:build": "astro build --adapter @astrojs/node",
    "test:e2e:serve": "node dist/server/entry.mjs",
    "// --- Utilities ---": "",
    "kb:gen": "find . -type f -not -path '*/node_modules/*' -not -path '*/.git/*' -not -path '*/dist/*' -not -path '*/.astro/*' -not -path '*/.vercel/*' -not -path '*/playwright-report/*' -not -path '*/test-results/*' -not -path '*/src/assets/*' -not -name 'pnpm-lock.yaml' -not -name 'knowledge_base.txt' -not -name '.DS_Store' -not -name '.env*' \\( -name '*.astro' -o -name '*.svelte' -o -name '*.ts' -o -name '*.tsx' -o -name '*.js' -o -name '*.mjs' -o -name '*.cjs' -o -name '*.json' -o -name '*.css' -o -name '*.md' -o -name '*.yml' -o -name '*.yaml' \\) -print0 | sort -z | xargs -0 -I {} sh -c 'echo \"\\n\\n================================\\n  FILE: {} \\n================================\\n\" && cat \"{}\"' > knowledge_base.txt",
    "gen:types": "tsx scripts/generate-types.ts",
    "prebuild": "pnpm gen:types",
    "test:parity": "vitest run tests/unit/schema-parity.test.ts"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.5",
    "@astrojs/db": "^0.18.3",
    "@astrojs/mdx": "^4.3.12",
    "@astrojs/node": "^9.5.1",
    "@astrojs/partytown": "^2.1.4",
    "@astrojs/sitemap": "^3.6.0",
    "@astrojs/svelte": "^7.2.2",
    "@astrojs/vercel": "^9.0.1",
    "@storyblok/astro": "^7.3.6",
    "@storyblok/svelte": "^5.2.13",
    "astro": "^5.16.0",
    "astro-seo-schema": "^5.1.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.554.0",
    "svelte": "^5.44.0",
    "tailwind-merge": "^3.4.0",
    "vite": "^5.4.11",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@astrojs/ts-plugin": "^1.10.5",
    "@faker-js/faker": "^10.1.0",
    "@playwright/test": "^1.57.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@tailwindcss/vite": "4.1.17",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/svelte": "5.2.9",
    "@types/node": "^24.10.1",
    "@types/wait-on": "^5.3.4",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@vitest/ui": "^4.0.14",
    "chalk": "^5.6.2",
    "dotenv": "^17.2.3",
    "esbuild": "^0.27.0",
    "eslint": "^9.39.1",
    "eslint-plugin-astro": "^1.5.0",
    "eslint-plugin-svelte": "^2.46.0",
    "jsdom": "27.2.0",
    "path-to-regexp": "^8.3.0",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "start-server-and-test": "^2.1.3",
    "storyblok-js-client": "^7.2.0",
    "svelte-check": "^4.3.4",
    "tailwind-variants": "^3.2.2",
    "tailwindcss": "4.1.17",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "vite-plugin-mkcert": "^1.17.9",
    "vitest": "^4.0.14",
    "wait-on": "^9.0.3"
  }
}
