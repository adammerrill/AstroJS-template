{
  "name": "projects-astro-js-template",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev:host": "astro dev --host 127.0.0.1 --port 4321",
    "dev": "astro dev --host 127.0.0.1 --port 4321",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "// --- Code Quality & Safety ---": "",
    "astro": "astro",
    "sync": "astro sync",
    "check": "astro check",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx,.astro .",
    "format": "prettier --write .",
    "clean": "rm -rf dist .astro node_modules",
    "// --- Testing Pipelines ---": "",
    "test": "pnpm test:e2e",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "// --- CI/CD Workflows ---": "",
    "test:ci": "tsx ./scripts/ci-wrapper.ts",
    "validate": "pnpm check && pnpm typecheck && pnpm build && pnpm test:ci",
    "pretest:e2e": "mkdir -p playwright-report",
    "// --- Generate a Knowledgebase ---": "",
    "kb:gen": "find . -type f -not -path '*/node_modules/*' -not -path '*/.git/*' -not -path '*/dist/*' -not -path '*/.astro/*' -not -path '*/.vercel/*' -not -path '*/playwright-report/*' -not -path '*/test-results/*' -not -path '*/src/assets/*' -not -name 'pnpm-lock.yaml' -not -name 'knowledge_base.txt' -not -name '.DS_Store' -not -name '.env*' \\( -name '*.astro' -o -name '*.svelte' -o -name '*.ts' -o -name '*.tsx' -o -name '*.js' -o -name '*.mjs' -o -name '*.cjs' -o -name '*.json' -o -name '*.css' -o -name '*.md' -o -name '*.yml' -o -name '*.yaml' \\) -print0 | sort -z | xargs -0 -I {} sh -c 'echo \"\\n\\n================================\\n  FILE: {} \\n================================\\n\" && cat \"{}\"' > knowledge_base.txt"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.5",
    "@astrojs/db": "^0.18.3",
    "@astrojs/mdx": "^4.3.12",
    "@astrojs/node": "^9.5.1",
    "@astrojs/partytown": "^2.1.4",
    "@astrojs/sitemap": "^3.6.0",
    "@astrojs/svelte": "^7.2.2",
    "@astrojs/vercel": "^9.0.1",
    "@storyblok/astro": "^7.3.6",
    "@storyblok/svelte": "^5.2.13",
    "astro": "^5.16.0",
    "astro-seo-schema": "^5.1.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.554.0",
    "svelte": "^5.44.0",
    "tailwind-merge": "^3.4.0",
    "vite": "^6.0.0"
  },
  "devDependencies": {
    "@astrojs/ts-plugin": "^1.10.5",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/vite": "4.1.17",
    "@types/node": "^24.10.1",
    "@types/wait-on": "^5.3.4",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "chalk": "^5.6.2",
    "eslint": "^9.39.1",
    "eslint-plugin-astro": "^1.5.0",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "start-server-and-test": "^2.1.3",
    "tailwindcss": "4.1.17",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
    "wait-on": "^9.0.3"
  }
}
